 <div class="col-md-4 col-md-offset-4">
   <% if current_user %>
    <%= link_to "Log Out", logout_path, class: "btn btn-default" %>
    <%= link_to "Manage Devices", user_path(current_user), class: "btn btn-default" %>
     <% else %>
     <h2>Login</h2>
     <%= form_for(:session_data, {url: sessions_path, class:
       "form-group"}) do |f| %>
         <%= f.label :email_address %>:
         <%= f.email_field :email_address, class: "form-control", :required => true %>

         <%= f.label :password %>:
         <%= f.password_field :password, class: "form-control", :required => true %><br>

         <%= f.submit "Login", class: "btn btn-success" %>      <%=link_to "Sign Up", new_user_path, class: "btn btn-primary"%>
     <% end %>
  <% end %>
</div>
 </div>
 </div>
